#sum of odd numbers

n = int(input("enter a numberr"))
sum = 0
for i in range (1,n+1):
    if i%2==1:
        sum = (sum+i)
        print(sum)
        
        
 #sum of even numbers
 n = int(input("enter a numberr"))
sum = 0
for i in range (1,n+1):
    if i%2==0:
        sum = (sum+i)
        print(sum)
